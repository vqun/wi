{
  "name": "wi",
  "version": "1.0.0",
  "description": "wi is a lite framework",
  "repository": "git@github.com:vqun/wi.git",
  "author": "vqun <veequn@qq.com>",
  "maintainers": [
    "veequn@qq.com"
  ],
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./bootstrap/webpack.config.js --mode production",
    "build:acc": "./node_modules/.bin/cross-env NODE_ENV=acc ./node_modules/.bin/webpack --config ./bootstrap/webpack.config.js --mode production",
    "build:dev": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/webpack --config ./bootstrap/webpack.config.js --mode production",
    "start": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/webpack --config ./bootstrap/webpack.config.js --mode development --watch",
    "serve": "./node_modules/.bin/hs -c 0 -p 1000 ./local"
  },
  "dependencies": {
    "ark": "https://github.com/vqun/ark.git",
    "es5-shim": "^4.5.10",
    "es6-promise": "^4.2.4",
    "jquery": "^3.3.1",
    "json3": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-loader": "^8.0.5",
    "c-loader": "^1.1.0",
    "config": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.29.6"
  },
  "optionalDependencies": {
    "http-server": "^0.11.1",
    "webpack-cli": "^3.1.0"
  }
}
